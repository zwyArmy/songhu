<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>	  
	  <rule>
		<from>^/fore/([a-z]+).html$</from>
		<to type= "forward" >/fore/$1.jsp</to>
	  </rule>
	  <rule>
		<from>^/fore/([a-zA-Z0-9]+)_([0-9]+).html$</from>
		<to type= "forward" >/fore/single.jsp?id=$1&amp;cumd=$2</to>
	  </rule>	  
      <rule>
		<from>^/fore/([a-z]+),([0-9]+),([0-9]+).html$</from>
		<to type= "forward" >/fore/$1.jsp?cumd=$2&amp;parentId=$3</to>
	  </rule>
	  <!-- 文章分页 -->
	  <rule>
		<from>^/fore/([a-zA-Z0-9]+),([0-9]+),([0-9]+),([0-9]+).html$</from>
		<to type= "forward" >/fore/$1.jsp?cumd=$2&amp;parentId=$3&amp;pages=$4</to>
	  </rule>
</urlrewrite>

