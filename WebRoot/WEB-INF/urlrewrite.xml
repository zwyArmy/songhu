<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>	  
	  <rule>
		<from>^/fore/([a-z]+).html$</from>
		<to type= "forward" >/fore/$1.jsp</to>
	  </rule>
	  <rule>
		<from>^/fore/([a-zA-Z0-9]+).html$</from>
		<to type= "forward" >/fore/detail.jsp?id=$1</to>
	  </rule>	  
	  <rule>
		<from>^/fore/([a-z]+)_([0-9]+).html$</from>
		<to type= "forward" >/fore/$1.jsp?columnId=$2</to>
	  </rule>
	  
	   <rule>
		<from>^/mobile/([a-z]+).html$</from>
		<to type= "forward" >/mobile/$1.jsp</to>
	  </rule>
	  <rule>
		<from>^/mobile/([a-zA-Z0-9]+).html$</from>
		<to type= "forward" >/mobile/detail.jsp?id=$1</to>
	  </rule>	  
	  <rule>
		<from>^/mobile/([a-z]+)_([0-9]+).html$</from>
		<to type= "forward" >/mobile/$1.jsp?columnId=$2</to>
	  </rule>
</urlrewrite>

